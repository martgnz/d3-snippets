".source.js, .source.ts, .source.tsx, .source.jsx":
    "Create a queue to load multiple files":
        prefix: "queue"
        body: """
        d3.queue()
            .defer(${1:d3.json}, '${2:}')
            .defer(${3:d3.json}, '${4:}')
            .await(function(error, ${5:file1}, ${6:file2}) {
                console.log(${7:file1}, ${8:file1});
            });
        """
